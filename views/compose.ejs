<%- include("partials/header") -%>

    <form class="item" action="/compose" method="post">
        <div class="form-group">
            <label for="title">Title</label>
            <input class="form-control" type="text" id="title" name="title" autocomplete="on>

        </div>
        <div class=" form-group">
            <label for="post">Post</label>
            <textarea class="form-control" rows="10" id="post" name="item"></textarea>

        </div>

        <button type="submit" class="btn btn-primary" name="btn">Publish</button>
    </form>



    <%- include("partials/footer") -%>